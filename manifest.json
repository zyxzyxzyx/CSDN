{  
  "name": "CSDN页面优化",
  "manifest_version":2,
  "version": "4.4.1",  
  "description": "自动显示博客全文，去掉底部登录长条和广告",
  "icons":{
    "128": "images/icon128.png"
  },
  "browser_action": {  
    "default_icon": "./images/icon128.png",
    "default_title": "interest"
  },
  "content_scripts": [
    {
      "matches": ["https://blog.csdn.net/*"],
      "js": ["main.js"]
    }
  ]
}
